from queue import (Queue, enQueue, deQueue, empty_Queue, destroy_Queue, integer)
from ctypes import (c_void_p, byref)

queue = Queue(type=integer, front=None, rear=None)
val = 10
val_type = c_void_p
enQueue(byref(queue), val_type(val))
print queue.front.contents.data
print deQueue(byref(queue))
empty_Queue(byref(queue)) 
destroy_Queue(byref(queue)) #this is crashing after emptying the queue successfully, invalid pointer - apparently
